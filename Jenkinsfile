pipeline {
  agent any 
  stages {
    stage("build") {
       steps {
          echo 'Realizando build da aplicação...'
          echo 'Build realizado com sucesso!'
       }
    }
    stage("test") {
       steps {
          echo 'Realizando testes da aplicação...'
       }
    }
    stage("deploy") {
       steps {
          echo 'Realizando deploy da aplicação...'
       }
    }
  }
}
